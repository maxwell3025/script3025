    "files.associations": {
        "*.py.typed": "python",
        "*.py.bak": "python",
        "*.fsh": "glsl",
        "*.vsh": "glsl",
        "unordered_set": "cpp",
        "memory": "cpp",
        "unordered_map": "cpp"
